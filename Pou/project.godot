config_version=4

[application]

config/name="Pou"
run/main_scene="res://scenes/states/Kitchen.tscn"
config/icon="res://icon.png"

[autoload]

Status="*res://scripts/status.gd"
FridgeContent="*res://scripts/fridge_content.gd"
States="*res://scripts/States.gd"

[display]

window/size/width=720
window/size/height=1280
window/size/resizable=false
window/size/borderless=true
window/size/test_width=480
window/size/test_height=854
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[input]

left_click={
"deadzone": 0.5,
"events": [ Object(InputEventMouseButton,"resource_local_to_scene":false,"resource_name":"","device":-1,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"button_mask":0,"position":Vector2( 0, 0 ),"global_position":Vector2( 0, 0 ),"factor":1.0,"button_index":1,"pressed":false,"doubleclick":false,"script":null)
 ]
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.392157, 0.54902, 0.878431, 1 )
environment/default_environment="res://default_env.tres"
